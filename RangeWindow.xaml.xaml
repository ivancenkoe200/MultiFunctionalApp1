<Window x:Class="MultiFunctionalApp.RangeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Определение диапазона" Height="200" Width="300">
    <StackPanel>
        <TextBlock Text="Введите число:" Margin="10"/>
        <TextBox Name="NumberInput" Width="100" Margin="10" />
        <Button Content="Проверить диапазон" Click="CheckRangeButton_Click" />
        <Label Name="RangeResult" Margin="10" />
    </StackPanel>
</Window>